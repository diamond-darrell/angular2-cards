{
  "name": "angular2-cards",
  "version": "0.5.0",
  "description": "Angular 2 Cards App",
  "scripts": {
    "start": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run json-server\" \"npm run server.prod\"",
    "server.dev": "webpack-dev-server --inline --progress --profile --colors --hot --watch --display-error-details --display-cached --port 8080",
    "server.prod": "node server.js",
    "build.prod": "rimraf dist && ./node_modules/.bin/webpack --config config/webpack.prod.js --progress --profile --bail",
    "build.dev": "rimraf dist && ./node_modules/.bin/webpack --progress --profile --bail",
    "json-server": "node ./node_modules/json-server/bin --watch db.json",
    "postinstall": "npm run build.prod"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.1",
    "core-js": "^2.4.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12",
    "angular2-uuid": "^1.0.7",
    "bootstrap": "^3.3.6",
    "bootstrap-sass": "^3.3.6",
    "ejs": "^2.4.2",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "express": "^4.14.0",
    "jquery": "^2.1.4"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "bootstrap-loader": "^1.0.10",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "imports-loader": "^0.6.5",
    "json-server": "^0.8.16",
    "node-sass": "^3.8.0",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.0",
    "script-loader": "^0.6.1",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  }
}
