{
  "extends": [
    "airbnb",
    "plugin:jasmine/recommended",
    "plugin:protractor/recommended"
  ],
  "plugins": [
    "import",
    "jasmine",
    "protractor"
  ],
  "env": {
    "browser": true,
    "jasmine": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "settings": {
    "import/resolver": "webpack"
  },
  "rules": {
    "new-cap": "off",
    "global-require": "off",
    "yoda": "off",
    "no-trailing-spaces": "off",
    "import/no-unresolved": "off"
  }
}